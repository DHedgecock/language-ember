{
  ".source.js.jsx": {
    "editor": {
      "commentStart": "// ",
      "foldEndPattern": "^\\s*\\}|^\\s*\\]|^\\s*\\)",
      "increaseIndentPattern": "{[^}\"']*$|\\[[^\\]\"']*$|\\([^)\"']*$|<[a-zA-Z][^/]*$|^\\s*>$",
      "decreaseIndentPattern": "^\\s*(\\s*/[*].*[*]/\\s*)*[}\\])]|^\\s*(</|/>)",
      "nonWordCharacters": "/\\()\"':,.;<>~!@#%^&*|+=[]{}`?-â€¦"
    }
  },
  ".source.inside-js.css.styled": {
    "editor": {
    "commentStart": "${''/* ",
    "commentEnd": "*/}"
    }
  },
  ".meta.tag.jsx": {
    "editor": {
      "commentStart": "{/* ",
      "commentEnd": " */}"
    }
  },
  ".JSXAttrs": {
    "editor": {
      "commentStart": "// "
    }
  },
  ".meta.embedded.expression.commented-out.jsx": {
    "editor": {
